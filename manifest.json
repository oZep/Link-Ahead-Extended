{
  "manifest_version": 3,
  "name": "Link Ahead",
  "version": "1.0",
  "description": "Identifying Phishing Links in Real Time",
  "author": "Joey Issa",
  "permissions": [
    "activeTab",
    "storage",
    "<all_urls>",
    "scripting",
    "tabs",
    "action",
    "https://*/*"

  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
