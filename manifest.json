{
    "manifest_version": 3,
    "name": "Link Ahead",
    "version": "1.0",
    "description": "Identifying Phishing Links in Real Time",
    "author": "Joey Issa",
    "permissions": [
        "activeTab",
        "storage",
        "https://*/*",
        "http://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    


    "icons": 
        {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
        },

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content_script.js"],
        "css": ["styles.css"]
      }
    ]
}

